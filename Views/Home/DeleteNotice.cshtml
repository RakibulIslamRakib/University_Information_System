@model Notice

<link rel="stylesheet" href="~/css/projects.css"/>
<link rel="stylesheet" href="~/css/contact.css" />

@{
    <div class="container">
        <h3 style="text-align:center">Delete Title: </h3>
        <form asp-controller="Home" asp-action="DeleteNotice">
            <label for="fname">Title</label>
            <input type="text" id="fname" disabled asp-for="NoticeTitle" placeholder="title..">


            <label for="subject">Descryption</label>
            <textarea id="subject" disabled asp-for="Descryption" placeholder="Write something.." style="height:200px"></textarea>
                <input type="submit" style="background-color:red; color:white;" value="Confirm Delete">
            </form>
    </div>
             
}